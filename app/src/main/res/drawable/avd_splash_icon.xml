<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="960dp"
            android:height="960dp"
            android:viewportWidth="960"
            android:viewportHeight="960">
            <group
                android:name="group"
                android:pivotX="480"
                android:pivotY="480"
                android:scaleX="0.7"
                android:scaleY="0.7">
                <group
                    android:name="group_right_paranthese"
                    android:pivotX="900"
                    android:pivotY="720"
                    android:translateX="-50"
                    android:scaleX="0"
                    android:scaleY="0"/>
                <path
                    android:name="right_parathese"
                    android:pathData="M 763 761 L 706 704 Q 752 659 776 600.5 Q 800 542 800 478 Q 800 414 775.5 355.5 Q 751 297 706 252 L 763 195 Q 819 252 849.5 325 Q 880 398 880 478 Q 880 558 850 631 Q 820 704 763 761 Z"
                    android:fillColor="#2d3069"/>
                <group
                    android:name="group_left_paranthese"
                    android:pivotX="200"
                    android:pivotY="700"
                    android:translateX="300"
                    android:scaleX="0"
                    android:scaleY="0"/>
                <path
                    android:name="left_paranthese"
                    android:pathData="M 197 761 Q 141 704 110.5 631 Q 80 558 80 478 Q 80 398 110 325 Q 140 252 197 195 L 254 252 Q 208 297 184 355.5 Q 160 414 160 478 Q 160 542 184.5 600.5 Q 209 659 254 704 L 197 761 Z"
                    android:fillColor="#2d3069"/>
                <group
                    android:name="group_dot"
                    android:pivotX="360"
                    android:pivotY="700"
                    android:translateX="100"
                    android:scaleX="0"
                    android:scaleY="0"/>
                <path
                    android:name="dot"
                    android:pathData="M 380 720 Q 355 720 337.5 702.5 Q 320 685 320 660 Q 320 635 337.5 617.5 Q 355 600 380 600 Q 405 600 422.5 617.5 Q 440 635 440 660 Q 440 685 422.5 702.5 Q 405 720 380 720 Z"
                    android:fillColor="#2d3069"/>
                <group
                    android:name="group_star"
                    android:pivotX="550"
                    android:pivotY="380"
                    android:translateX="-100"
                    android:scaleX="0"
                    android:scaleY="0">
                    <path
                        android:name="star"
                        android:pathData="M 519 520 L 519 449 L 458 485 L 418 415 L 479 380 L 418 345 L 458 275 L 519 311 L 519 240 L 599 240 L 599 311 L 660 275 L 700 345 L 639 380 L 700 415 L 660 485 L 599 449 L 599 520 L 519 520 Z"
                        android:fillColor="#2d3069"/>
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group_left_paranthese">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="150"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="150"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="100"
                    android:duration="400"
                    android:valueFrom="300"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="left_paranthese">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:duration="200"
                    android:valueFrom="#2687fd"
                    android:valueTo="#9c26fd"
                    android:valueType="colorType"
                    android:interpolator="@android:anim/anticipate_interpolator"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="200"
                    android:duration="200"
                    android:valueFrom="#9c26fd"
                    android:valueTo="#fd9c26"
                    android:valueType="colorType"
                    android:interpolator="@android:anim/bounce_interpolator"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="400"
                    android:duration="100"
                    android:valueFrom="#fd9c26"
                    android:valueTo="#2d3069"
                    android:valueType="colorType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_dot">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="200"
                    android:duration="300"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="200"
                    android:duration="300"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="300"
                    android:duration="300"
                    android:valueFrom="100"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_overshoot_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="dot">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="200"
                    android:duration="200"
                    android:valueFrom="#87fd26"
                    android:valueTo="#9c26fd"
                    android:valueType="colorType"
                    android:interpolator="@android:anim/bounce_interpolator"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="400"
                    android:duration="200"
                    android:valueFrom="#9c26fd"
                    android:valueTo="#fd9c26"
                    android:valueType="colorType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="600"
                    android:duration="200"
                    android:valueFrom="#fd9c26"
                    android:valueTo="#2d3069"
                    android:valueType="colorType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_star">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="300"
                    android:duration="200"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="300"
                    android:duration="200"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="300"
                    android:duration="200"
                    android:valueFrom="-100"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="300"
                    android:duration="700"
                    android:valueFrom="-270"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_right_paranthese">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="500"
                    android:duration="200"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="500"
                    android:duration="200"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="600"
                    android:duration="200"
                    android:valueFrom="-50"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_overshoot_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="right_parathese">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="500"
                    android:duration="200"
                    android:valueFrom="#9c26fd"
                    android:valueTo="#87fd26"
                    android:valueType="colorType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="700"
                    android:duration="100"
                    android:valueFrom="#87fd26"
                    android:valueTo="#2687fd"
                    android:valueType="colorType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="800"
                    android:duration="100"
                    android:valueFrom="#2687fd"
                    android:valueTo="#2d3069"
                    android:valueType="colorType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="star">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="300"
                    android:duration="100"
                    android:valueFrom="#2687fd"
                    android:valueTo="#2687fd"
                    android:valueType="colorType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="800"
                    android:duration="100"
                    android:valueFrom="#2687fd"
                    android:valueTo="#2d3069"
                    android:valueType="colorType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="400"
                    android:duration="100"
                    android:valueFrom="#87fd26"
                    android:valueTo="#87fd26"
                    android:valueType="colorType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="500"
                    android:duration="100"
                    android:valueFrom="#fd9c26"
                    android:valueTo="#fd9c26"
                    android:valueType="colorType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="600"
                    android:duration="100"
                    android:valueFrom="#9c26fd"
                    android:valueTo="#9c26fd"
                    android:valueType="colorType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="fillColor"
                    android:startOffset="700"
                    android:duration="100"
                    android:valueFrom="#2687fd"
                    android:valueTo="#2687fd"
                    android:valueType="colorType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
